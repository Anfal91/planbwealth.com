                <div class="row">
                    <div class="col-xl-12 col-lg-12 col-md-12 col-sm-4">
                        <div class="contact-form">
                            <form action="/send_mail" method="post" id="contact-form" data-toggle="validator"
                                role="form">
                                <div class="form-group">
                                    <input id="inputName" type="text" class="form-control" name="inputname"
                                        placeholder="Name" required data-error="Enter your name">
                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="form-group">
                                    <input id="email" type="email" class="form-control" name="inputemail"
                                        placeholder="Email" required data-error="Enter your email">
                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="form-group">
                                    <input id="subject" type="text" class="form-control" name="inputsubject"
                                        placeholder="Subject" required data-error="Enter your subject">
                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="form-group">
                                    <textarea id="message" class="form-control" name="inputmessage"
                                        placeholder="Message" rows="5" required
                                        data-error="Enter message with details" style="resize:none"></textarea>
                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="btn-2">
                                    <div class="col-lg-12">
                                        <button id="mailBtn" class="btn-primary" name="submit-form"
                                            type="submit">Send</button>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-12">
                                        <div id="msgalert" class="hidden"></div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>

                </div>

            </div>

